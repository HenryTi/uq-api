Console {
    a {
        供应商信息;
        物流中心信息;
        采购单;
        b {
            采购单;
        }
    }
    供应商信息;
}

DUO 物流中心货品 {
    I 物流中心信息;
    X 货品信息;
};

OPTIONS 货品库存状态 (
    待审,
    待验,
    可售,
    待退,
);

DUO 机构物流中心 {
    I 机构信息;
    X 物流中心信息;
};

DUO 器械批次库存状态 {
    I 器械批次;
    X 货品库存状态;
};

-- (#物流中心货品(a,b).?);

REPORT 物流中心货品库存报表 {
    Permit *;
    From 物流中心货品 to 货品信息;
    Title (库存.可出数量 '可出数量',库存.数量 '库存数量');
};
